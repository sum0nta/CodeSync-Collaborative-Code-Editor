{
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/build",
  "installCommand": "cd frontend && npm install",
  "framework": "create-react-app",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://codesync-collaborative-code-editor-production.up.railway.app/api/$1"
    }
  ],
  "env": {
    "REACT_APP_BACKEND_URL": "https://codesync-collaborative-code-editor-production.up.railway.app"
  }
}
